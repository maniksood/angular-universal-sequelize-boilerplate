{
  "name": "angular-universal-sequelize",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "globalnpms": "npm install -g pm2 nodemon concurrently mysql mysql2 ts-node node-cmd",
    "server": "nodemon --delay 5 ./build/server/server.js",
    "server:nonstop": "cd ./build/server && pm2 start server.js",
    "watch:server": "tsc -p ./server -w",
    "watch:client": "ng build -w",
    "watch:all": "concurrently --kill-others \"tsc -p ./server -w\" \"ng build -w\" ",
    "build:server": "rm -r build/server && tsc -p ./server",
    "build:client": "rm -r build/client && ng build --prod ",
    "build:all": "rm -r build && tsc -p ./server && ng build --prod && ng build --prod --app 1 --output-hashing=none",
    "env": "source env.sh",
    "syncModels": "source ./env.sh && cd server/Storage && ts-node syncModels.ts",
    "build:ssr": "ng build --prod && ng build --prod --app 1 --output-hashing=none"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.1.1",
    "@angular/router": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "config": "^1.28.1",
    "core-js": "^2.4.1",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^8.1.0",
    "mysql2": "^1.5.1",
    "node-cmd": "^3.0.0",
    "rxjs": "^5.5.2",
    "sequelize": "^4.28.6",
    "serve-favicon": "^2.4.5",
    "ts-loader": "^3.2.0",
    "yaml": "^0.3.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.1",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
